{
    "name": "patrickmaken/kafka-log-driver",
    "description": "Laravel, log driver for Kafka",
    "keywords": [
        "patrickmaken",
        "laravel",
        "kafka",
        "log",
        "driver"
    ],
    "homepage": "https://github.com/patrickmaken/kafka-log-driver",
    "license": "MIT",
    "authors": [
        {
            "name": "Patrick Madeng",
            "email": "patrickmaken@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "ext-rdkafka": "^6.0",
        "illuminate/support": "^11.0",
        "illuminate/contracts": "^11.0",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Patrickmaken\\KafkaLogDriver\\": "src/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Patrickmaken\\KafkaLogDriver\\KafkaLogDriverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
